)\r\n      //----------\r\n      const handleSubmit = (e) => {\r\n        e.preventDefault();\r\n        console.log(formValue)\r\n      }\r\n\r\n      //\r\n\r\n    return (\r\n        <form onSubmit= {handleSubmit}>\r\n            <h1>FormWithCustomHook</h1>  \r\n            <hr/>\r\n\r\n            <div className=\"form-group\">\r\n                <input \r\n                    type=\"text\"\r\n                    name=\"name\"\r\n                    className=\"from-control\"\r\n                    placeholder=\"tu nombre\"\r\n                    autoCapitalize=\"off\"\r\n                    value={name}\r\n                    onChange={handleInputChange}\r\n                />\r\n            </div>\r\n\r\n\r\n            <div className=\"form-group\">\r\n                <input \r\n                    type=\"text\"\r\n                    name=\"email\"\r\n                    className=\"from-control\"\r\n                    placeholder=\"email@gmail.com\"\r\n                    autoCapitalize=\"off\"\r\n                    value={email}\r\n                    onChange={handleInputChange}\r\n                />\r\n            </div>\r\n\r\n            <div className=\"form-group\">\r\n                <input \r\n                    type=\"password\"\r\n                    name=\"password\"\r\n                    className=\"from-control\"\r\n                    placeholder=\"****\"\r\n                    value={password}\r\n                    onChange={handleInputChange}\r\n                />\r\n            </div>\r\n            <button type=\"submit\" className=\"btn btn-primary\">\r\n                Guardar\r\n            </button>\r\n        </form>\r\n    )\r\n}\r\n",["165","166"],"C:\\Users\\ARMAGEDOM\\Desktop\\CUROS REACT\\Trabajos\\React\\05-hook-app\\src\\hooks\\useForm.js",[],["167","168"],"C:\\Users\\ARMAGEDOM\\Desktop\\CUROS REACT\\Trabajos\\React\\05-hook-app\\src\\components\\03-examples\\MultipleCustomHooks.js",[],"C:\\Users\\ARMAGEDOM\\Desktop\\CUROS