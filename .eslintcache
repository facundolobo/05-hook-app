a\r\n        }\r\n        // dispatch\r\n        dispatch( action );\r\n    }   \r\n\r\n    //console.log(description);\r\n    const handleToggle=(todoId)=>{\r\n        dispatch({ //forma corta \r\n            type: 'toggle',\r\n            payload:todoId\r\n        })\r\n    }\r\n\r\n    const handleAddTodo=(newTodo)=>{\r\n        \r\n        dispatch( {\r\n            type:'add',\r\n            payload: newTodo\r\n        } );\r\n    }\r\n\r\n\r\n\r\n    return (\r\n        <div className=\"row\"> \r\n           <h1>TodoApp ({todos.length})</h1> \r\n           <hr/>\r\n            \r\n            <div className=\"col-7\">\r\n            \r\n                <TodoItem \r\n                todos={todos} \r\n                handleDelete = {handleDelete} \r\n                handleToggle={handleToggle} \r\n                />\r\n            \r\n            </div>\r\n            \r\n            <div className=\"col-5\">\r\n                <TodoAdd\r\n                handleAddTodo={handleAddTodo}\r\n                />\r\n            </div>\r\n\r\n            \r\n        </div>    \r\n        \r\n            \r\n\r\n    )\r\n}\r\n","C:\\Users\\ARMAGEDOM\\Desktop\\CUROS REACT\\Trabajos\\React\\05-hook-app\\src\\components\\08-useReducer\\todoReducer.js",[],"C:\\Users\\ARMAGEDOM\\Desktop\\CUROS REACT\\Trabajos\\React\\05-hook-app\\src\\components\\08-useReducer\\TodoItem.js",[],"C:\\Users\\ARMAGEDOM\\Desktop\\CUROS REACT\\Trabajos\\React\\05-hook-app\\src\\components\\08-useReducer\\TodoListItem.js",[],"C:\\Users\\ARMAGEDOM\\Desktop\\CUROS REACT\\Trabajos\\React\\05-hook-app\\src\\components\\08-useReducer\\TodoAdd.js",[],"C:\\Users\\ARMAGEDOM\\Desktop\\CUROS REACT\\Trabajos\\React\\05-hook-app\\src\\components\\09-useContext\\MainApp.js",[],"C:\\Users\\ARMAGEDOM\\Desktop\\CUROS REACT\\Trabajos\\React\\05-hook-app\\src\\components\\09-useContext\\AppRouter.js",[],"C:\\Users\\ARMAGEDOM\\Desktop\\CUROS REACT\\Trabajos\\React\\05-hook-app\\src\\components\\09-useContext\\AboutScr